CREATE TABLE  First(
NOMSKLAD int NULL,
NOMDOG int NULL,
NOMDOK int NULL,
KODDET int NULL,
KOL int NULL,
DATAP DATE)
GO
ALTER TABLE  First
ADD CONSTRAINT ck_movies_check_date_date>01.03.2004
CHECK ("YEAR"AND"DATAP">'2004-03-01');
GO
ALTER TABLE First ADD PRIMARY KEY(NOMSKLAD,NOMDOK);
GO


CREATE TABLE  Second(
NOMDOG int NULL,
KODDET int NULL,
DTN DATE,
DTK DATE, 
PLAN int NULL,
DOGSTOIM int
)
GO
ALTER TABLE  Second
ADD CONSTRAINT ck_movies_check_date_dtn>01.01.2004
CHECK ("YEAR"AND"DTN">'2004-01-01');
GO
ALTER TABLE  Second
ADD CONSTRAINT ck_movies_check_date_DTK>DTN
CHECK ("YEAR"AND"DTK">"YEAR"AND"DTN");
GO
ALTER TABLE Second ADD PRIMARY KEY(KODDOG,NOMDOG);
GO


CREATE TABLE  Third(
KODDET int NULL,
TYPE int NULL,
NAMEDET NVARCRAR(50),
PLANSTOIM int)

ADD CONSTRAINT ck_movies_check_PLANSTOIM>0
CHECK (PLANSTOIM>0);
GO
ALTER TABLE Third ADD PRIMARY KEY(KODDOG);
GO
